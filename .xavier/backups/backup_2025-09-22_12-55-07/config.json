{
  "name": "chrome-linkedin",
  "version": "1.0.0",
  "xavier_version": "1.1.5",
  "settings": {
    "strict_mode": true,
    "test_first": true,
    "clean_code_enforcement": true,
    "ioc_patterns": true,
    "sequential_execution": true,
    "test_coverage_required": 100,
    "sprint_velocity": 20,
    "default_sprint_duration": 14,
    "auto_update_check": true
  },
  "agents": {
    "project_manager": {
      "enabled": true
    },
    "context_manager": {
      "enabled": true
    },
    "python_engineer": {
      "enabled": true
    },
    "golang_engineer": {
      "enabled": true
    },
    "frontend_engineer": {
      "enabled": true
    },
    "devops": {
      "enabled": true
    }
  }
}